{
  "manifest_version": 2,

  "name": "Wide Github",
  "description": "Change all Github repository and gist pages to be full width and dynamically sized.",
  "version": "1.2.1",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*", "https://gist.github.com/*", "https://git.ci.bbryiot.com/*"],
      "css": ["wide-github.css"]
    }
  ]
}
